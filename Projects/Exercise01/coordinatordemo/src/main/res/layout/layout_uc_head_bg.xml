<?xml version="1.0" encoding="utf-8"?>
<lzhs.com.coordinatordemo.widget.DisInterceptNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_collapseMode="parallax"
    android:clipChildren="false"
    android:clipToPadding="false"
    app:layout_collapseParallaxMultiplier="0.8">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="@dimen/app_bar_height"
            android:background="@mipmap/ic_avater"
            android:scaleType="centerCrop"
            android:tag="overScroll" />

    </LinearLayout>

</lzhs.com.coordinatordemo.widget.DisInterceptNestedScrollView>